{
  "name": "faker-django",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/style.css --minify",
    "watch": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/style.css --watch",
    "vercel-build": "npm run build && python manage.py collectstatic --noinput"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.31"
  }
}
